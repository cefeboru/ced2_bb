<schema name="lnoh_ced_response">

  <table name="lnoh_ced_response">
    <comment>WebService responses</comment>

    <column name="id_pk1" data-type="int" nullable="false" identity="true" />
    <column name="student_rut" data-type="char(12)" nullable="false" />
    <column name="course_id" data-type="char(20)" nullable="false" />
    <column name="assistance_date" data-type="int" nullable="false" />
